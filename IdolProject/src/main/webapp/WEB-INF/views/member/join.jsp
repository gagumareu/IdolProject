		<input type="checkbox" name="fav_celeb[]" id="celeb_${dto.getCeleb_no() }" value="${dto.getCeleb_no() }" onchange="check_celeb(${dto.getCeleb_no()})" > 
		<label for="celeb_${dto.getCeleb_no() }" >
       <span class="celeb_span"> ${dto.getCeleb_name() } </span>
       	</label>
		</div>
		</c:if>
		
